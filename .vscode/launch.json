{
    "version": "0.2.0",
    "configurations": [
        {
            "args": [],
            "cwd": "${workspaceRoot}",
            "env": {
                "NODE_ENV": "development"
            },
            "name": "DEBUG",
            "outFiles": ["${workspaceRoot}/bin/**/*.js"],
            "preLaunchTask": "compile",
            "program": "${workspaceRoot}/src/main.ts",
            "request": "launch",
            "runtimeArgs": [
                "--nolazy"
